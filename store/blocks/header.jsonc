{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "sticky-layout#desktop"
    ]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": [
      "flex-layout.row#headerLine1",
      "flex-layout.row#headerLine2"
    ]
  },
  "flex-layout.row#headerLine1": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "middle",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "search-bar",
      "minicart.v2"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "logo"
    ]
  },
  "logo": {
    "props": {
      "title": "Upmedal Logo",
      "href": "/",
      "url": "assets/upmedal-logo.png",
      "width": 150,
      "height": 35.7,
      "mobileWidth": 80,
      "mobileHeight": 19.4
    }
  },
  "flex-layout.row#headerLine2": {
    "props": {
      "horizontalAlign": "left",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "vtex.menu@2.x:menu#categories"
    ]
  },
  "vtex.menu@2.x:menu#categories": {
    "props": {
      "items": [
        {
          "id": "menu-item-promocao",
          "highlight": false,
          "itemProps": {
            "text": "Promoção"
          }
        },
        {
          "id": "menu-item-corinthians",
          "highlight": true,
          "itemProps": {
            "text": "Corinthians"
          }
        },
        {
          "id": "menu-item-palmeiras",
          "highlight": true,
          "itemProps": {
            "text": "Palmeiras"
          }
        },
        {
          "id": "menu-item-signos",
          "highlight": true,
          "itemProps": {
            "text": "Signos"
          }
        },
        {
          "id": "menu-item-divertidos",
          "highlight": true,
          "itemProps": {
            "text": "Divertidos"
          }
        },
        {
          "id": "menu-item-7maravilhas",
          "highlight": true,
          "itemProps": {
            "text": "7 Maravilhas"
          }
        },
        {
          "id": "menu-item-guerreiros",
          "highlight": true,
          "itemProps": {
            "text": "Guerreiros"
          }
        }
      ]
    }
  },
  "minicart.v2": {
    "props": {
      "MinicartIcon": "icon-cart#header"
    }
  },
  "icon-cart#header": {
    "props": {
      "size": 24,
      "viewBox": "0 0 24 24"
    }
  },
  "header-layout.mobile": {
    "children": [
      "sticky-layout#mobile"
    ]
  },
  "sticky-layout#mobile": {
    "children": [
      "flex-layout.row#mobile"
    ]
  },
  "flex-layout.row#mobile": {
    "children": [
      "logo",
      "search-bar",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  }
}